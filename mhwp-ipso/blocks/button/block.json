{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "mhwp-ipso/button",
  "title": "ipsobutton",
  "description": "Defenieer een ipso reserverings button",
  "category": "text",
  "keywords": [ "ipso" ],
  "supports": {
    "html": false
  },
  "style": "file:../dist/button/mhwp-ipso-button.css",
  "editorScript": "file:../dist/button/mhwp-ipso-button.js",
  "viewScript": "file:../dist/button/mhwp-ipso-button-frontend.js",
  "attributes": {
    "activity_id": {
      "type": "number",
      "default": 0,
      "source": "attribute",
      "selector": "#activity_id",
      "attribute": "value"
    },
    "activity_date": {
      "type": "string",
      "default": "",
      "source": "attribute",
      "selector": "#activity_date",
      "attribute": "value"
    },
    "activity_title": {
      "type": "string",
      "default": "",
      "source": "attribute",
      "selector": "#activity_title",
      "attribute": "value"
    }
  }
}
